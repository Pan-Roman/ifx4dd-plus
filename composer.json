{
    "name": "pan-roman/ifx4dd-plus",
    "description": "Informix Platform for Doctrine DBAL",
    "keywords": ["informix", "doctrine", "dbal", "database", "persistence", "queryobject"],
    "homepage": "https://github.com/Pan-Roman/ifx4dd-plus",
    "license": "MIT",
    "authors": [
        {"name": "Jose M. Alonso M. Roman P.", "email": "romio@inbox.ru", "role": "monkey hitting keys"}
    ],
    "require": {
        "ext-pdo_informix": ">=1.3.1",
        "doctrine/dbal": "2.5.12"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^6.3",
        "symfony/console": "2.*||^3.0"
    },
    "autoload": {
        "psr-0": { "Doctrine\\DBAL\\": "lib/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml*", "build.xml"]
    }
}
